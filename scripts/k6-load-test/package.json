{
  "name": "k6-load-test",
  "version": "1.0.1",
  "description": "Load test for Trial System scenarios",
  "main": "/dist/index.js",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.config.js --mode=production",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "exec": "npm run build && dotenv -e .env k6 run --http-debug=\"full\" dist/index.js"
  },
  "devDependencies": {
    "@babel/core": "7.13.16",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@pagopa/openapi-codegen-ts": "^13.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/k6": "~0.45.0",
    "@types/webpack": "5.28.0",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^9.2.0",
    "eslint-config-rules": "workspace:^",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.4.5",
    "util": "^0.12.5",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-glob-entries": "^1.0.1"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@mattrglobal/http-signatures": "^4.1.0",
    "@pagopa/ts-commons": "^13.0.0",
    "date-fns": "^2.30.0",
    "fp-ts": "^2.16.5",
    "io-ts": "^2.2.19",
    "io-ts-types": "^0.5.19",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5"
  }
}
